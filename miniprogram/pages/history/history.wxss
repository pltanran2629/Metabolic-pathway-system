.content{
  padding-top: 15vh;
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.compoundcard{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  margin-bottom: 2vh;
}
.compoundcardname{
  font-size: 36rpx;
  margin-bottom: 1vh;
}
.compoundcarddes{
  letter-spacing: 0.1em;
  line-height: 1.5em;
  font-size: 28rpx;
}
.pathwayname{
  font-size: 35rpx;
  margin-top: 1.5vh;
  color: #47484b;
  text-align: center;
}